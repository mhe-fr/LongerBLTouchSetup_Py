<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="title" translatable="yes">Longer LK4 pro BLTouch Setup</property>
    <property name="width">600</property>
    <bind sequence="&lt;Destroy&gt;" handler="close_app" add="" />
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="minsize">100</property>
        </containerlayout>
        <child>
          <object class="ttk.Entry" id="entry_com" named="True">
            <property name="state">normal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_init" named="True">
            <property name="command" type="command" cbtype="simple">on_click_init</property>
            <property name="text" translatable="yes">Init</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_get_z" named="True">
            <property name="command" type="command" cbtype="simple">on_click_get_z</property>
            <property name="text" translatable="yes">Get Z</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="entry_z" named="True">
            <property name="state">normal</property>
            <property name="takefocus">true</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_save_z" named="True">
            <property name="command" type="command" cbtype="simple">on_click_save_z</property>
            <property name="text" translatable="yes">Save Z</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_goto_z0" named="True">
            <property name="command" type="command" cbtype="simple">on_click_goto_z0</property>
            <property name="text" translatable="yes">Goto Z0</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_open_com" named="True">
            <property name="command" type="command" cbtype="simple">on_click_open_com</property>
            <property name="default">normal</property>
            <property name="state">normal</property>
            <property name="text" translatable="yes">Open COM</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_goto_z10" named="True">
            <property name="command" type="command" cbtype="simple">on_click_goto_z10</property>
            <property name="text" translatable="yes">Goto Z10</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Text" id="console" named="True">
            <property name="background">#000000</property>
            <property name="exportselection">false</property>
            <property name="foreground">#5fe262</property>
            <property name="height">10</property>
            <property name="insertunfocussed">none</property>
            <property name="state">disabled</property>
            <property name="width">50</property>
            <property name="wrap">none</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
